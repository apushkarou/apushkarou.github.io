{
  "name": "project-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "develop": "cross-env NODE_ENV=development webpack serve --config webpack/webpack.dev.js",
    "deploy": "gh-pages -d build",
    "test:jest": "cross-env NODE_ICU_DATA=node_modules/full-icu jest",
    "test:jest:coverage": "npm run test:jest -- --coverage",
    "test:editorconfig": "editorconfig-checker --exclude '.git|.husky|node_modules|dist'",
    "test:eslint": "eslint 'src/**/*.js' --config .eslintrc.js",
    "test:eslint:fix": "npm run test:eslint -- --fix",
    "test:prettier": "prettier --check 'src/**/*.js'",
    "test:prettier:fix": "npm run test:prettier -- --write .",
    "clean": "rimraf build",
    "pre-commit": "lint-staged",
    "pre-push": "npm run test:jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.18.2",
    "@testing-library/jest-dom": "^5.16.4",
    "autoprefixer": "^10.4.8",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^28.1.1",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.1",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "file-loader": "^6.2.0",
    "full-icu": "^1.4.0",
    "gh-pages": "^4.0.0",
    "glob-all": "^3.3.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.16",
    "postcss-loader": "^7.0.1",
    "postcss-nested": "^5.0.6",
    "prettier": "2.7.1",
    "purgecss-webpack-plugin": "^4.1.3",
    "rimraf": "^3.0.2",
    "sass": "^1.54.5",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "lint-staged": {
    "*.js": [
      "npm run test:editorconfig",
      "npm run test:prettier:fix",
      "npm run test:eslint:fix"
    ]
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "bootstrap": "^5.2.0"
  }
}
